{
    "name": "smoothsystem/manager",
    "description": "SmoothSystem[TM]",
    "type": "library",
    "authors": [
        {
            "name": "Yudi Hertanto",
            "email": "y.hertanto17@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "laravel/framework": "^5.8|^6.2",
        "laravel/passport": "^7.5|^8.3",
        "fzaninotto/faker": "^1.4",
        "langleyfoxall/math_eval": "^2.0",
        "superbalist/laravel-google-cloud-storage": "^2.2",
        "owen-it/laravel-auditing": "^10.0",
        "nicolaslopezj/searchable": "1.*",
        "wildside/userstamps": "^1.1",
        "staudenmeir/eloquent-json-relations": "^1.3.2"
    },
    "autoload": {
        "psr-4": {
            "Smoothsystem\\Manager\\": "src/"
        },
        "files": []
    },
    "extra": {
		"branch-alias": {
            "dev-master": "master-dev",
            "dev-v1": "1.0.x-dev",
            "dev-v2": "2.0.x-dev"
		},
        "laravel": {
            "providers": [
                "Smoothsystem\\Manager\\ManagerServiceProvider"
            ],
            "aliases": {
                "ExceptionService": "Smoothsystem\\Manager\\Utilities\\Facades\\ExceptionService",
                "FileService": "Smoothsystem\\Manager\\Utilities\\Facades\\FileService",
                "RouteService": "Smoothsystem\\Manager\\Utilities\\Facades\\RouteService",
                "StubService": "Smoothsystem\\Manager\\Utilities\\Facades\\StubService"
            }
        }
    }
}
